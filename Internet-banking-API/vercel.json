{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["./**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ],
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "startCommand": "pnpm run start"
}

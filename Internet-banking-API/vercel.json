{
  "version": 2,
  "builds": [
    {
      "src": "./dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["./**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "outputDirectory": "dist"
}
